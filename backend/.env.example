# Logging
LOG_LEVEL=INFO
LOG_FORMAT=simple  # simple or json
LOG_FILE_PATH=logs/backend.log
LOG_ROTATION=5 MB
LOG_RETENTION=1 month

# CORS
ALLOWED_HOSTS=["*"] 