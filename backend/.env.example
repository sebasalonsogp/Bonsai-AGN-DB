# Environment
ENVIRONMENT=development
DEBUG=true

# Database
DB_HOST=localhost
DB_PORT=3306
DB_USER=agndb_user
DB_PASSWORD=agndb_password
DB_NAME=agndb
DB_ECHO_LOG=false
DB_POOL_SIZE=5
